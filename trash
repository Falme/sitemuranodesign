		createLine('Nilton','Fernando');

		createLine('Carol','Rafael');

		createLine('Rafael','Fernando');
		createLine('Rafael','Vitor');

		createLine('Bruno','Pedro');
		createLine('Bruno','Fabio' , "Bruno");
		createLine('Bruno','Alessandra');

		createLine('Pedro','Fernando');
		createLine('Pedro','Fabio');



		createLine('Fernando','Fabio');
		createLine('Fernando','Vitor');

