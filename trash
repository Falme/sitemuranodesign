	/* Aparecer Menu No Modo Mobile */

	$('#MenuIconXs').click(function(){
		ChangeStatusMenuXs()
	});

	function ChangeStatusMenuXs()
	{
		$('#MenuXs').css('top', (MenuMobileAtivo ? '-300px':'75px'));
		MenuMobileAtivo = (MenuMobileAtivo ? false:true);
	}





