//No Desktop, Quem se Liga com quem (InicioLinha, FimLinha, [Linha deve sumir para aparecer o texto ao clica em quem])

	function LinesHD()
	{
		createLine('Julia','Vitor');
		createLine('Julia','Nilton');
		createLine('Julia','Lucas');

		createLine('Vitor','Diego', "Vitor");
		createLine('Vitor','Nilton');

		createLine('Diego','Nilton', 'Dora');
		createLine('Diego','Dora', 'Dora');
		//createLine('Diego','Felipe');

		//createLine('Felipe','Dora');
		//createLine('Felipe','Alessandra','Felipe');

		createLine('Lucas','Rafael');
		//createLine('Lucas','Fabio', 'Lucas Rafael Fabio');

		createLine('Nilton','Rafael');
		createLine('Nilton','Pedro');
		createLine('Nilton','Dora', 'Dora');

		createLine('Dora','Pedro');
		createLine('Dora','Carol');
		createLine('Dora','Alessandra');

		createLine('Alessandra','Carol');
		createLine('Alessandra','Fernando', 'Alessandra Carol Fernando');

		//createLine('Rafael','Fabio');
		createLine('Rafael','Pedro','Pedro');
		createLine('Rafael','Bruno');

		createLine('Pedro','Bruno');
		createLine('Pedro','Rogerio');
		createLine('Pedro','Carol');

		createLine('Carol','Rogerio');
		createLine('Carol','Fernando');

		createLine('Fabio','Diego');
		createLine('Fabio','Dora');
		createLine('Fabio','Alessandra');

		createLine('Bruno','Rogerio','Bruno');

		createLine('Rogerio','Fernando', 'Rogerio');

	}

	//No Tablet, Quem se Liga com quem (InicioLinha, FimLinha, [Linha deve sumir para aparecer o texto ao clica em quem])
	function LinesTab()
	{

		createLine('Julia','Dora');
		createLine('Julia','Nilton', "Nilton");
